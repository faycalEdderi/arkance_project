    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>


   


       <table class="table table-striped">
        <tr>
            <th>Classe</th>
            <th>Professeur Principal</th>
           
        </tr>
        {% for teacherInfo in teacher %}
        <tr>
            {% if teacherInfo.headSchoolClass.level is defined and teacherInfo.headSchoolClass.level is not null %} 
            <td> {{ teacherInfo.headSchoolClass.level }}</td>
            <td> {{ teacherInfo.firstName }}</td>
        
        {% endif %}
                     
        </tr>
     


        {% endfor %}
             

       

       

   
  




</div>
{% endblock %}
